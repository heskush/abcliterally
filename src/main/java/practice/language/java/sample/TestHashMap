import java.util.*

public class Test{
    public static void main(String[]args){
        HashMap<Integer,String>map=new HashMap<>();
        map.put(1,"one");
        map.put(2,"two");
        map.put(3,"three");
        Set<Map.Entry<Integer,String>> entrySet=map.entrySet();
        Iterator itr=entrySet.iterator();
        while(itr.hasNext()){
            System.out.println("THE NEXT VALUE IS "+itr.next())
        }
    }

}